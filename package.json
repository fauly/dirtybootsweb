    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.3.5"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31",
    "postcss-cli": "^11.0.1"
  },
  "keywords": []
}
